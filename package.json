{
  "name": "madosayentisuto",
  "version": "1.0.0",
  "description": "Hey, mister... I am mad scientist! It's so cool! Sonuvabitch.",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "clean": "rm -fr dist/*",
    "prebuild": "yarn run checkEnv && yarn run clean",
    "build": "tsc -p tsconfig.build.json && parcel build src/client/index.html --dist-dir dist/client",
    "checkEnv": "ts-node src/server/checkEnv.ts",
    "server": "ts-node src/server/index.ts",
    "~server": "nodemon -e js,ts,json -x \"yarn run server\"",
    "~client": "parcel src/client/index.html --dist-dir dist/client",
    "prettier": "prettier --check src test",
    "prettierFix": "prettier --write src test",
    "lint": "eslint --cache --max-warnings 0 src test",
    "lintFix": "eslint --fix --cache --max-warnings 0 src test",
    "jestUnit": "jest --testPathPattern=.*\\.spec\\.ts",
    "jestCoverage": "jest --ci --coverage",
    "style": "yarn run prettier && yarn run lint",
    "test": "yarn run style && yarn run dc && yarn run jestUnit",
    "dc": "depcruise --config -- src",
    "dcGraph": "depcruise -x '(node_modules|fs|util|fp\\.ts)' -T dot src | dot -T svg -Gsplines=ortho | depcruise-wrap-stream-in-html > dependencygraph.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gurimarukin/madosayentisuto.git"
  },
  "author": "Gurimarukin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Gurimarukin/madosayentisuto/issues"
  },
  "homepage": "https://github.com/Gurimarukin/madosayentisuto#readme",
  "dependencies": {
    "@discordjs/builders": "^0.8.2",
    "@discordjs/rest": "^0.1.0-canary.0",
    "@discordjs/voice": "^0.7.5",
    "discord-api-types": "^0.24.0",
    "discord.js": "^13.3.1",
    "dotenv": "^10.0.0",
    "execa": "^5.0.0",
    "express": "^4.17.2",
    "fp-ts": "^2.11.5",
    "fp-ts-contrib": "^0.1.26",
    "fp-ts-routing": "^0.5.4",
    "fp-ts-rxjs": "^0.6.15",
    "history": "^5.2.0",
    "hyper-ts": "^0.7.6",
    "io-ts": "^2.2.16",
    "ky": "^0.28.7",
    "mongodb": "^4.1.3",
    "monocle-ts": "^2.3.11",
    "newtype-ts": "^0.3.4",
    "qs": "^6.10.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rxjs": "^6.6.7",
    "swr": "^1.1.2",
    "youtube-dl-exec": "^2.0.0"
  },
  "devDependencies": {
    "@discordjs/opus": "^0.7.0",
    "@trivago/prettier-plugin-sort-imports": "^3.1.0",
    "@type-config/strict": "^1.1.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.1",
    "@types/qs": "^6.9.7",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/shell-quote": "^1.7.1",
    "@types/ws": "^8.2.0",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "dependency-cruiser": "^10.5.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-fp-ts": "^0.2.1",
    "eslint-plugin-functional": "^3.7.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "jest": "^27.3.0",
    "nodemon": "^2.0.13",
    "parcel": "^2.0.1",
    "prettier": "^2.4.1",
    "redux": "^4.1.1",
    "shell-quote": "^1.7.2",
    "sodium": "^3.0.2",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.3.0",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4"
  }
