{
  "name": "madosayentisuto",
  "version": "1.0.0",
  "description": "Hey, mister... I am mad scientist! It's so cool! Sonuvabitch.",
  "main": "src/index.ts",
  "scripts": {
    "preinstall": "node -e \"!process.env.npm_config_user_agent.startsWith('pnpm/') && !console.log('Use \\`npx pnpm install\\` to install dependencies in this repository\\n') && process.exit(1)\"",
    "clean": "rm -fr dist/*",
    "prebuild": "npm run clean",
    "build": "tsc",
    "checkEnv": "ts-node src/checkEnv.ts",
    "compile": "tsc --noEmit",
    "~compile": "tsc --noEmit -w",
    "run": "ts-node src/index.ts",
    "~run": "nodemon -e js,ts,json -x \"npm run --silent run\"",
    "test": "jest",
    "~test": "jest --watchAll",
    "lint": "eslint --cache --fix --max-warnings 0 --ext .ts src/ test/",
    "dc:graph": "depcruise --include-only '^src' --exclude 'fp\\.ts' --output-type dot . | dot -T svg > dependencygraph.svg",
    "dc:osage": "depcruise --include-only '^src' --exclude 'fp\\.ts' --output-type dot . | osage -Gpack=16 -Gsplines=ortho -GpackMode=array2 -T svg > dependencygraph.svg"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gurimarukin/madosayentisuto.git"
  },
  "author": "Gurimarukin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Gurimarukin/madosayentisuto/issues"
  },
  "homepage": "https://github.com/Gurimarukin/madosayentisuto#readme",
  "devDependencies": {
    "@discordjs/collection": "^0.1.6",
    "@discordjs/opus": "^0.1.0",
    "@type-config/strict": "^1.0.2",
    "@types/dateformat": "^3.0.1",
    "@types/jest": "^25.2.3",
    "@types/mongodb": "^3.6.7",
    "@types/node": "^13.13.32",
    "@types/shell-quote": "^1.7.0",
    "@types/ws": "^7.4.0",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "dateformat": "^3.0.3",
    "dependency-cruiser": "^9.17.1",
    "discord.js": "^12.5.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "fp-ts": "^2.9.0",
    "fp-ts-contrib": "^0.1.21",
    "fp-ts-rxjs": "^0.6.11",
    "io-ts": "^2.2.13",
    "jest": "^25.5.4",
    "mongodb": "^3.6.4",
    "monocle-ts": "^2.3.3",
    "newtype-ts": "^0.3.4",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.0",
    "redux": "^4.0.5",
    "rxjs": "^6.6.3",
    "shell-quote": "^1.7.2",
    "ts-jest": "^25.5.1",
    "ts-node": "^8.10.2",
    "tslib": "^1.14.1",
    "typescript": "^3.9.7"
  }
