    "eslint-plugin-react": "7.34.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-tailwindcss": "3.15.1",
    "jest": "29.7.0",
    "nodemon": "3.1.0",
    "parcel": "2.12.0",
    "postcss": "8.4.38",
    "prettier": "3.2.5",
    "process": "0.11.10",
    "punycode": "^2.3.1",
    "querystring-es3": "0.2.1",
    "sodium": "3.0.2",
    "tailwindcss": "3.4.3",
    "ts-jest": "29.1.2",
    "tslib": "2.6.2",
    "typescript": "5.4.3",
    "url": "0.11.3"
  }
}
