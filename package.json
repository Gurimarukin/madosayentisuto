{
  "name": "madosayentisuto",
  "version": "1.0.0",
  "description": "Hey, mister... I am mad scientist! It's so cool! Sonuvabitch.",
  "targets": {
    "server": {
      "source": "src/server/index.ts",
      "engines": {
        "node": ">= 18"
      }
    },
    "client": {
      "source": "src/client/index.html",
      "engines": {
        "browsers": "> 0.5%, last 2 versions, not dead"
      }
    }
  },
  "scripts": {
    "clean": "rm -fr .parcel-cache dist",
    "prebuild": "yarn run clean && yarn run checkEnv",
    "build": "tsc -p tsconfig.build.json && parcel build",
    "checkEnv": "ts-node -P tsconfig.ts-node.json src/server/checkEnv.ts && ts-node -P tsconfig.ts-node.json src/client/checkEnv.ts",
    "createUser": "ts-node -P tsconfig.ts-node.json src/server/createUser.ts",
    "server": "ts-node -P tsconfig.ts-node.json src/server/index.ts",
    "~server": "nodemon -e ts,tsx,js,json,env --ignore dist/ --ignore src/client/ -x \"yarn run server\"",
    "~client": "concurrently -k -n parcel,tsc -c green.bold,cyan.bold \"parcel src/client/index.html --no-autoinstall --dist-dir dist/client\" \"tsc -w --preserveWatchOutput -p tsconfig.client.json\"",
    "~compile": "tsc -w --preserveWatchOutput -p tsconfig.json",
    "prettier": "prettier --check src test",
    "prettierFix": "prettier --write src test",
    "lint": "eslint --cache --max-warnings 0 src test",
    "lintFix": "eslint --fix --cache --max-warnings 0 src test",
    "jestUnit": "jest --testPathPattern=.*\\.spec\\.ts",
    "jestCoverage": "jest --ci --coverage",
    "style": "yarn run prettier && yarn run lint",
    "test": "yarn run style && yarn run dc && yarn run jestUnit",
    "dc": "depcruise --config -- src",
    "dcGraph": "depcruise -x '(node_modules|fs|util|fp\\.ts)' -T dot src | dot -T svg -Gsplines=ortho | depcruise-wrap-stream-in-html > dependencygraph.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gurimarukin/madosayentisuto.git"
  },
  "author": "Gurimarukin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Gurimarukin/madosayentisuto/issues"
  },
  "homepage": "https://github.com/Gurimarukin/madosayentisuto#readme",
  "dependencies": {
    "@discordjs/voice": "0.16.0",
    "@napi-rs/canvas": "0.1.39",
    "@phc/argon2": "1.0.9",
    "cookie": "0.5.0",
    "dayjs": "1.11.7",
    "discord.js": "14.11.0",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "fast-deep-equal": "3.1.3",
    "fp-ts": "2.13.1",
    "fp-ts-contrib": "0.1.26",
    "fp-ts-routing": "0.5.4",
    "fp-ts-rxjs": "0.6.15",
    "got": "11.8.6",
    "history": "5.3.0",
    "hyper-ts": "0.7.8",
    "io-ts": "2.2.20",
    "jsonwebtoken": "9.0.0",
    "ky": "0.33.3",
    "mongodb": "5.4.0",
    "monocle-ts": "2.3.13",
    "ms": "3.0.0-canary.1",
    "newtype-ts": "0.3.5",
    "qs": "6.11.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "reconnecting-websocket": "4.4.0",
    "redux": "4.2.1",
    "rxjs": "6.6.7",
    "rxjs-stream": "3.3.0",
    "swr": "1.3.0",
    "upash": "1.0.2",
    "uuid": "9.0.0",
    "ws": "8.13.0",
    "youtube-dl-exec": "2.4.0"
  },
  "devDependencies": {
    "@discordjs/opus": "0.9.0",
    "@trivago/prettier-plugin-sort-imports": "4.1.1",
    "@type-config/strict": "1.2.1",
    "@types/cookie": "0.5.1",
    "@types/express": "4.17.17",
    "@types/jest": "29.5.0",
    "@types/jsonwebtoken": "9.0.1",
    "@types/node": "18.11.9",
    "@types/qs": "6.9.7",
    "@types/react": "17.0.42",
    "@types/react-dom": "17.0.14",
    "@types/uuid": "9.0.1",
    "@typescript-eslint/eslint-plugin": "5.59.2",
    "@typescript-eslint/parser": "5.59.2",
    "autoprefixer": "10.4.14",
    "concurrently": "7.6.0",
    "dependency-cruiser": "12.11.0",
    "eslint": "8.40.0",
