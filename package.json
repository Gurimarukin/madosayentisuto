{
  "name": "madosayentisuto",
  "version": "1.0.0",
  "description": "Hey, mister... I am mad scientist! It's so cool! Sonuvabitch.",
  "type": "module",
  "targets": {
    "server": {
      "source": "src/server/index.ts",
      "context": "node",
      "engines": {
        "node": ">= 20"
      },
      "outputFormat": "esmodule"
    },
    "client": {
      "source": "src/client/index.html",
      "context": "browser",
      "engines": {
        "browsers": "> 0.5%, last 2 versions, not dead"
      }
    }
  },
  "scripts": {
    "_node": "node --loader=./node_modules/commonjs-extension-resolution-loader/loader.js",
    "_swc": "swc src --out-dir tsbuild",
    "clean": "rm -fr .parcel-cache dist tsbuild",
    "prebuild": "yarn clean",
    "build": "yarn _swc && yarn _node tsbuild/src/server/checkEnv.js && yarn _node tsbuild/src/client/checkEnv.js && parcel build",
    "createUser": "yarn _swc && yarn _node tsbuild/src/server/createUser.js",
    "swcServer": "yarn _swc && yarn _node tsbuild/src/server/index.js",
    "~swcServer": "nodemon -e ts,tsx,js,json,env --ignore dist/ --ignore tsbuild/ --ignore src/client/ -x \"yarn swcServer\"",
    "~server": "concurrently -k -n swc,tsc -c green.bold,cyan.bold \"yarn ~swcServer\" \"yarn ~compile -p tsconfig.server.json\"",
    "~client": "concurrently -k -n parcel,tsc -c green.bold,cyan.bold \"parcel src/client/index.html --no-autoinstall --dist-dir dist/client\" \"yarn ~compile -p tsconfig.client.json\"",
    "~compile": "tsc -w --preserveWatchOutput",
    "_prettier": "prettier src test *.json *.md *.cjs",
    "formatCheck": "yarn _prettier --check",
    "format": "yarn _prettier --write",
    "lint": "eslint --cache --max-warnings 0 src test",
    "lintFix": "yarn lint --fix",
    "test": "jest",
    "testUnit": "yarn test --testPathPattern=.*\\.spec\\.ts",
    "testCoverage": "yarn test --ci --coverage",
    "dc": "depcruise src test",
    "dcGraph": "depcruise -v -T dot src -x '(node_modules|fs|util|fp\\.ts)' | dot -T svg | depcruise-wrap-stream-in-html > dependencygraph.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gurimarukin/madosayentisuto.git"
  },
  "author": "Gurimarukin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Gurimarukin/madosayentisuto/issues"
  },
  "homepage": "https://github.com/Gurimarukin/madosayentisuto#readme",
  "dependencies": {
    "@discordjs/voice": "0.16.1",
    "@napi-rs/canvas": "0.1.51",
    "@phc/argon2": "1.0.9",
    "cookie": "0.6.0",
    "dayjs": "1.11.10",
    "discord.js": "14.14.1",
    "dotenv": "16.4.5",
    "express": "4.19.2",
    "fast-deep-equal": "3.1.3",
    "fp-ts": "2.16.5",
    "fp-ts-contrib": "0.1.29",
    "fp-ts-routing": "0.6.0",
    "fp-ts-rxjs": "0.6.15",
    "history": "5.3.0",
    "hyper-ts": "0.7.9",
    "io-ts": "2.2.21",
    "jsonwebtoken": "9.0.2",
    "ky": "1.2.3",
    "mongodb": "6.5.0",
    "monocle-ts": "2.3.13",
    "ms": "3.0.0-canary.1",
    "newtype-ts": "0.3.5",
    "qs": "6.12.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "reconnecting-websocket": "4.4.0",
    "redux": "4.2.1",
    "rxjs": "6.6.7",
    "rxjs-stream": "3.3.0",
    "swr": "2.2.5",
    "upash": "1.0.2",
    "uuid": "9.0.1",
    "ws": "8.16.0",
    "youtube-dl-exec": "2.5.7"
  },
  "devDependencies": {
    "@discordjs/opus": "0.9.0",
    "@swc/cli": "0.3.12",
    "@swc/core": "1.4.11",
    "@swc/jest": "0.2.36",
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@types/cookie": "0.6.0",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.12",
    "@types/jsonwebtoken": "9.0.6",
    "@types/node": "20.12.2",
    "@types/qs": "6.9.14",
    "@types/react": "18.2.73",
    "@types/react-dom": "18.2.23",
    "@types/uuid": "9.0.8",
    "@typescript-eslint/eslint-plugin": "7.5.0",
    "@typescript-eslint/parser": "7.5.0",
    "autoprefixer": "10.4.19",
    "commonjs-extension-resolution-loader": "0.1.0",
    "concurrently": "8.2.2",
    "dependency-cruiser": "16.2.4",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-deprecation": "2.0.0",
    "eslint-plugin-fp-ts": "0.3.2",
