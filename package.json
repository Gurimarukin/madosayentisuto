{
  "name": "madosayentisuto",
  "version": "1.0.0",
  "description": "Hey, mister... I am mad scientist! It's so cool! Sonuvabitch.",
  "main": "src/index.ts",
  "scripts": {
    "clean": "rm -fr dist/*",
    "prebuild": "yarn run checkEnv && yarn run clean",
    "build": "tsc -p ./tsconfig.build.json",
    "checkEnv": "ts-node src/checkEnv.ts",
    "run": "ts-node src/index.ts",
    "~run": "nodemon -e js,ts,json -x \"yarn run run\"",
    "prettier": "prettier --check \"./{src,test}/**/*.ts\"",
    "prettierFix": "prettier --write \"./{src,test}/**/*.ts\"",
    "importSort": "import-sort --list-different \"./{src,test}/**/*.ts\"",
    "importSortFix": "import-sort --write \"./{src,test}/**/*.ts\"",
    "lint": "eslint --cache --max-warnings 0 --ext .ts src test",
    "lintFix": "eslint --fix --cache --max-warnings 0 --ext .ts src test",
    "jest": "jest",
    "jestCoverage": "jest --ci --coverage",
    "style": "yarn run prettier && yarn run importSort && yarn run lint",
    "test": "yarn run style && yarn run jest",
    "dcGraph": "depcruise --include-only '^src' --exclude 'fp\\.ts' --output-type dot . | dot -T svg > dependencygraph.svg",
    "dcOsage": "depcruise --include-only '^src' --exclude 'fp\\.ts' --output-type dot . | osage -Gpack=16 -Gsplines=ortho -GpackMode=array2 -T svg > dependencygraph.svg"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gurimarukin/madosayentisuto.git"
  },
  "author": "Gurimarukin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Gurimarukin/madosayentisuto/issues"
  },
  "homepage": "https://github.com/Gurimarukin/madosayentisuto#readme",
  "devDependencies": {
    "@discordjs/builders": "^0.8.2",
    "@type-config/strict": "^1.1.1",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.1",
    "@types/shell-quote": "^1.7.1",
    "@types/ws": "^8.2.0",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "dependency-cruiser": "^10.5.0",
    "discord.js": "^13.3.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-fp-ts": "^0.2.1",
    "eslint-plugin-functional": "^3.7.2",
    "eslint-plugin-prettier": "^4.0.0",
    "fp-ts": "^2.11.5",
    "fp-ts-rxjs": "^0.6.15",
    "import-sort-cli": "^6.0.0",
    "import-sort-parser-typescript": "^6.0.0",
    "import-sort-style-module": "^6.0.0",
    "io-ts": "^2.2.16",
    "jest": "^27.3.0",
    "mongodb": "^4.1.3",
    "monocle-ts": "^2.3.11",
    "newtype-ts": "^0.3.4",
    "nodemon": "^2.0.13",
    "prettier": "^2.4.1",
    "redux": "^4.1.1",
    "rxjs": "^6.6.7",
    "shell-quote": "^1.7.2",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.3.0",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4"
  },
  "importSort": {
    ".js, .ts": {
      "style": "module",
      "parser": "typescript"
    }
  }
}
