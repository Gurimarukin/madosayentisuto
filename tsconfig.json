{
  "extends": "@type-config/strict",
  "compilerOptions": {
    "outDir": "dist",
    "incremental": true,
    "sourceMap": true,
    "noEmit": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "paths": {
      "bot/*": ["./src/bot/*"],
      "shared/*": ["./src/shared/*"]
    }
  },
  "include": ["src", "test"]
}
